<!doctype html>
<html ng-app="fuse" ng-strict-di>

<head>
	<base href="./">
	<meta charset="utf-8">
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<title>MND Patient Dashboard</title>
	<link type="text/css" rel="stylesheet" href="assets/roboto.css" />
</head>

<body md-theme="{{vm.themes.active.name}}" ng-controller="IndexController as vm" class="{{state.current.bodyClass || ''}}">
	<!-- SPLASH SCREEN -->
	<ms-splash-screen id="splash-screen">
		<div class="center">
			<div class="logo">
				<span>MND</span>
			</div>
			<!-- Material Design Spinner -->
			<div class="spinner-wrapper">
				<div class="spinner">
					<div class="inner">
						<div class="gap"></div>
						<div class="left">
							<div class="half-circle"></div>
						</div>
						<div class="right">
							<div class="half-circle"></div>
						</div>
					</div>
				</div>
			</div>
			<!-- / Material Design Spinner -->
		</div>
	</ms-splash-screen>
	<!-- / SPLASH SCREEN -->
	<div id="main" class="animate-slide-up" ui-view="main" layout="column"></div>
	<!-- Scripts -->
</body>

</html>
