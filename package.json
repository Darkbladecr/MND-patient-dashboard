{
  "name": "MND-Patient-Dashboard",
  "productName": "MND Patient Dashboard",
  "author": "Stefan Mitrasinovic",
  "version": "0.1.0",
  "private": true,
  "description": "Dashboard for MND patients",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "webpack": "BABEL_ENV=development NODE_ENV=development webpack-dev-server --open --hot",
    "webpack:dev": "BABEL_DISABLE_CACHE=1 BABEL_ENV=development NODE_ENV=development webpack --display-modules --progress",
    "webpack:prod": "BABEL_ENV=development NODE_ENV=production webpack --display-modules --progress",
    "webpack:prod-win": "webpack --display-modules --progress",
    "webpack:stats": "webpack --json > stats.json",
    "build": "npm run webpack:prod && npm run copy-files",
    "build-backend": "BABEL_ENV=server babel server -d distServer",
    "copy-files": "cp -R src/assets build",
    "package": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.sysmed.MNDDashboard",
    "icon": "src/icon.icns",
    "dmg": {
      "icon": "src/icon.icns"
    },
    "win": {
      "target": "portable",
      "icon": "src/icon.ico"
    },
    "files": [
      "main.js",
      "backend${/*}",
      "build${/*}",
      "!(src|logs|server|distServer|dist)${/*}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkbladecr/ques.git"
  },
  "license": "ISC",
  "bugs": {
